COPY START(1),LENGTH(1) TO $8000
COPY START(2),LENGTH(2) TO $FF00

REM INIT SPRITES
SPRITE.A 0,(6,,,,1)
SPRITE.A 1,(1,,,,1)

DIM X(1),Y(1)
X(0)=60
Y(0)=64
X(1)=100
Y(1)=64
DO
  FOR I=0 TO 1
    SPRITE I,,,130
    IF UP(I) THEN Y(I)=Y(I)-2
    IF DOWN(I) THEN Y(I)=Y(I)+2
    IF LEFT(I) THEN X(I)=X(I)-2
    IF RIGHT(I) THEN X(I)=X(I)+2
    IF BUTTON(I,0) THEN SPRITE I,,,132
    IF BUTTON(I,1) THEN SPRITE I,,,134
    SPRITE I,X(I)+24,Y(I)+24,
  NEXT I
  WAIT VBL
LOOP


